# VALGRIND_READY - to mark the test ready for valgrindTestSuite.sh

--NAME--
Convop query compound attributes by valueID (XML)
--SHELL-INIT--
source harnessFunctions.sh

dbInit CB
brokerStart CB

--SHELL--
echo "1: +++++++++ create vector compound +++++++++++"
(curl localhost:${BROKER_PORT}/NGSI10/contextEntities/EVector -s -S --header 'Content-Type: application/xml' -d @- | xmllint --format - ) <<EOF
<?xml version="1.0" encoding="UTF-8"?>
<appendContextElementRequest>
  <contextAttributeList>
    <contextAttribute>
      <name>A</name>
      <type>T</type>
      <contextValue type="vector">
        <item>22</item>
        <item>
          <x type="vector">
            <item>x1</item>
            <item>x2</item>
          </x>
          <y>3</y>
        </item>
        <item type="vector">
          <item>z1</item>
          <item>z2</item>
        </item>
      </contextValue>
      <metadata>
        <contextMetadata>
          <name>ID</name>
          <type>string</type>
          <value>ground</value>
        </contextMetadata>
      </metadata>
    </contextAttribute>
    <contextAttribute>
      <name>A</name>
      <type>T</type>
      <contextValue type="vector">
        <item>23</item>
        <item>
          <x type="vector">
            <item>x3</item>
            <item>x4</item>
          </x>
          <y>4</y>
        </item>
        <item type="vector">
          <item>z3</item>
          <item>z4</item>
        </item>
      </contextValue>
      <metadata>
        <contextMetadata>
          <name>ID</name>
          <type>string</type>
          <value>wall</value>
        </contextMetadata>
      </metadata>
    </contextAttribute>
  </contextAttributeList>
</appendContextElementRequest>
EOF

echo "2: +++++++++ create object compound +++++++++++"
(curl localhost:${BROKER_PORT}/NGSI10/contextEntities/EObject -s -S --header 'Content-Type: application/xml' -d @- | xmllint --format - ) <<EOF
<?xml version="1.0" encoding="UTF-8"?>
<appendContextElementRequest>
  <contextAttributeList>
    <contextAttribute>
      <name>A</name>
      <type>T</type>
      <contextValue>
        <x>
          <x1>a</x1>
          <x2>b</x2>
        </x>
        <y type="vector">
          <item>y1</item>
          <item>y2</item>
        </y>
      </contextValue>
      <metadata>
        <contextMetadata>
          <name>ID</name>
          <type>string</type>
          <value>ground</value>
        </contextMetadata>
      </metadata>
    </contextAttribute>
    <contextAttribute>
      <name>A</name>
      <type>T</type>
      <contextValue>
        <x>
          <x1>c</x1>
          <x2>d</x2>
        </x>
        <y type="vector">
          <item>y3</item>
          <item>y4</item>
        </y>
      </contextValue>
      <metadata>
        <contextMetadata>
          <name>ID</name>
          <type>string</type>
          <value>wall</value>
        </contextMetadata>
      </metadata>
    </contextAttribute>
  </contextAttributeList>
</appendContextElementRequest>
EOF

echo "3: ++++++++++ query for vector compound by valueID 'ground' ++++++++++"
curl localhost:${BROKER_PORT}/NGSI10/contextEntities/EVector/attributes/A/ground -s -S --header 'Accept: application/xml' | xmllint --format -

echo "4: ++++++++++ query for vector compound by valueID 'wall' ++++++++++"
curl localhost:${BROKER_PORT}/NGSI10/contextEntities/EVector/attributes/A/wall -s -S --header 'Accept: application/xml' | xmllint --format -

echo "5: ++++++++++ query for object compound by valueID 'ground' ++++++++++"
curl localhost:${BROKER_PORT}/NGSI10/contextEntities/EObject/attributes/A/ground -s -S --header 'Accept: application/xml' | xmllint --format -

